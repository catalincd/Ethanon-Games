class GameOverManager : GameObject{	InterpolationTimer@ mainTimer = InterpolationTimer(5000); 	InterpolationTimer@ backTimer = InterpolationTimer(750); 	InterpolationTimer@ textTimer = InterpolationTimer(750); 		uint gameOverTime;	vector2 textPos;	void create()	{		gameOverTime = 0;		mainTimer.reset(5000);		backTimer.reset(1250);		textTimer.reset(750);		textPos = vector2(SS_50.x, SS_50.y);	}			void update()	{		if(GetInputHandle().GetKeyState(K_2) == KS_HIT) GAME_OVER = true;			if(GAME_OVER && !GAME_SUCCESS)		{			if(gameOverTime==0)				gameOverTime = getTimeAbsolute();			mainTimer.update();						if(mainTimer.getElapsed() > 1250)			{				backTimer.update();				uint op = 255.0f * backTimer.getBias();				uint color = ARGB(op,0,0,0);				DrawShapedSprite("sprites/square.png", V2_ZERO, GetScreenSize(), color);								if(mainTimer.getElapsed() > 2000)				{					textTimer.update();					uint op = 255.0f * textTimer.getBias();					uint color = ARGB(op,255,40,20);					DrawText("MISSION FAILED", text128, textPos, color, V2_HALF, 0.75f);				}												if(getTimeAbsolute() - gameOverTime > 5000)				{					g_sceneManager.setCurrentScene(MainMenuScene());					g_sceneManager.transitionBack();				}							}					}	}		void resume()	{		/*			go to the main scene		*/	}	string getTag(){ return "GameOver"; }}GameOverManager g_gameOver;